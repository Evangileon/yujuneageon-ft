<?xml version="1.0" ?>
<GUIScheme Name="LineSpace">
    <Imageset Name="LineSpace" Filename="LineSpace.imageset" />
    <Font Name="BlueHighway-12" Filename="bluehighway-12.font" />
    <Font Name="BlueHighway-10" Filename="bluehighway-10.font" />
    <Font Name="BlueHighway-8" Filename="bluehighway-8.font" />

    <LookNFeel Filename="linespace.looknfeel" />
    <LookNFeel Filename="static.looknfeel" />
    <LookNFeel Filename="scroll.looknfeel" />
    <LookNFeel Filename="containers.looknfeel" />
    <LookNFeel Filename="lists.looknfeel" />
    <LookNFeel Filename="menu.looknfeel" />
    <LookNFeel Filename="progress.looknfeel" />
    <LookNFeel Filename="targeting.looknfeel" />
    <LookNFeel Filename="tabcontrol.looknfeel" />

    <WindowRendererSet Filename="CEGUIFalagardWRBase" />

