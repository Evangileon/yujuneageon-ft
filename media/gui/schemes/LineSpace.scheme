    <FalagardMapping WindowType="LineSpace/ComboEditbox" TargetType="CEGUI/Editbox" Renderer="Falagard/Editbox" LookNFeel="LineSpace/ComboEditbox" />
    <FalagardMapping WindowType="LineSpace/ComboDropList" TargetType="CEGUI/ComboDropList" Renderer="Falagard/Listbox" LookNFeel="LineSpace/ComboDropList" />
    <FalagardMapping WindowType="LineSpace/MultiColumnList" TargetType="CEGUI/MultiColumnList" Renderer="Falagard/MultiColumnList" LookNFeel="LineSpace/MultiColumnList" />
