material Terrain_Azul
{
    receive_shadows on
    technique
    {
        // Wireframe component of terrain
        pass
        {
            polygon_mode wireframe
            ambient 0 0 0 1
            diffuse 0 0 0 1
        }

        // Filled-in polygons of terrain
        pass
        {
            ambient 0.0 0.0 0.0 0.0
            diffuse 0.000000 0.445312 1.000000 1.000000
            specular 1.000000 1.000000 1.000000 1.000000 33.750000
        }
    }
}
